Music VARCHAR(30) PRIMARY KEY,
Fans varchar(100),
Artists varchar(50),
Venues varchar(40),
City varchar(100),
Promoters varchar(30),
Inclusivity varchar(25),
Impact varchar(30)
);

CREATE TABLE OneStopExperience(
Fans VARCHAR(200) PRIMARY KEY,
Tickets varchar(100),
Concerts varchar(50),
Music varchar(40),
Artists varchar(100),
City varchar(30)
);

CREATE TABLE Recommendations(
Fans VARCHAR(200) PRIMARY KEY,
Venues varchar(100),
Concerts varchar(50),
Music varchar(40),
Artists varchar(100),
Geography varchar(30)
);

CREATE TABLE Artists(
Music VARCHAR(200) PRIMARY KEY,
Tour Dates date(100),
Announcements varchar(50),
Venues varchar(40),
Tickets varchar(100),
Genres varchar(30),
Live Streams varchar(50),
Promoters varchar(80)
);

CREATE TABLE User(
Fans VARCHAR(150) PRIMARY KEY,
Music varchar(150),
Geography varchar(70),
Tickets varchar(30),
Followed Artists varchar(120),
Concerts varchar(80),
Live Streams varchar(60)
);

CREATE TABLE ConcertsFestivals(
Music VARCHAR(175) PRIMARY KEY,
Tickets varchar(100),
Fans varchar(95),
Venues varchar(100),
Promoters varchar(30),
Live Streams varchar(20)
);

CREATE TABLE Venues(
Music VARCHAR(150) PRIMARY KEY,
Live Shows varchar(120),
Promoters varchar(40),
Tickets varchar(50),
City varchar(75),
Fans varchar(100)
);

