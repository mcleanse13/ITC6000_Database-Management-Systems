-- Select Statements
SELECT * FROM Artists;

-- Retrieve Concert Details with Venue Information
SELECT ConcertsFestivals.Live
FROM ConcertsFestivals
JOIN Venues ON ConcertsFestivals.Music = Venues.Music;

-- Select Fans from tables
SELECT DISTINCT Community.Fans, OneStopExperience.Fans, Recommendations.Fans, User.Fans, ConcertsFestivals.Fans, Venues.Fans
FROM Community, OneStopExperience, Recommendations, User, ConcertsFestivals, Venues;

-- Count Concerts by City
SELECT Venues.City, COUNT(OneStopExperience.Fans) AS ConcertCount
FROM Venues
LEFT JOIN OneStopExperience ON Venues.Music = OneStopExperience.Fans
GROUP BY Venues.City;

-- List Venues and Ticket Information
SELECT Artists.Tickets, Artists.Venues, ConcertsFestivals.Tickets, ConcertsFestivals.Venues
FROM Artists
JOIN ConcertsFestivals ON Artists.Music = ConcertsFestivals.Music
WHERE ConcertsFestivals.Tickets = 'General Admission';

-- Order By Function with Geography columns
SELECT Recommendations.Geography, User.Geography
FROM Recommendations, User
ORDER BY Recommendations.Geography ASC, user.Geography DESC;
